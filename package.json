{
  "name": "milesxwalkerstudio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint src --ext .vue,.js,.ts,.jsx,.tsx",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,json,css,scss,md}\" \"**/*.md\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,jsx,tsx,json,css,scss,md}\" \"**/*.md\" --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "check-files": "./docs/commands/检查文件分类.sh",
    "check-all": "npm run check-files && npm run lint:check && npm run format:check && npm run type-check",
    "fix-all": "npm run lint && npm run format",
    "vercel:dev": "vercel dev",
    "vercel:build": "vercel build",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel"
  },
  "dependencies": {
    "@tresjs/cientos": "^4.3.1",
    "@tresjs/core": "^4.3.5",
    "@vueuse/core": "^10.11.1",
    "core-js": "^3.36.0",
    "lucide-vue-next": "^0.303.0",
    "motion-v": "^1.1.0-alpha.1",
    "pinia": "^2.1.7",
    "three": "^0.177.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@iconify/vue": "^5.0.0",
    "@inspira-ui/plugins": "^0.0.1",
    "@stagewise/toolbar-vue": "^0.1.2",
    "@types/node": "^20.11.16",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue-tsc": "^2.2.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
